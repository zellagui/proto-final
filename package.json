{
  "name": "vulk",
  "version": "2.0.1",
  "private": true,
  "license": "SEE LICENSE IN LICENSE.md",
  "type": "module",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "scripts": {
    "dev": "vite --open",
    "build": "run-s clean build:*",
    "build:update-bulma-colors": "bulma-css-vars",
    "build:vite": "cross-env NODE_ENV=production vite build",
    "serve": "serve -s dist -p 3000",
    "ssr:dev": "jiti server.ts",
    "ssr:build": "run-s clean build:update-bulma-colors ssr:build:*",
    "ssr:build:client": "vite build --ssrManifest --outDir dist/client",
    "ssr:build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "ssr:serve": "cross-env NODE_ENV=production jiti server.ts",
    "ssg:build": "pnpm build:update-bulma-colors && cross-env NODE_ENV=production jiti build-ssg.ts",
    "ssg:serve": "serve dist -p 3000",
    "clean": "rimraf dist",
    "test": "run-p test:*",
    "test:tsc": "vue-tsc --noEmit",
    "test:lint": "run-s lint:eslint lint:stylelint",
    "lint": "run-s lint:prettier:fix lint:eslint:fix lint:stylelint:fix",
    "lint:prettier": "prettier --check \"./src/**/*.(ts|vue|css|scss)\"",
    "lint:prettier:fix": "prettier --write \"./src/**/*.(ts|vue|css|scss)\"",
    "lint:eslint": "eslint -c .eslintrc.cjs --ext .ts,.vue ./src",
    "lint:eslint:fix": "eslint -c .eslintrc.cjs --fix --ext .ts,.vue ./src",
    "lint:stylelint": "stylelint ./src/**/*.{vue,css,scss}",
    "lint:stylelint:fix": "stylelint ./src/**/*.{vue,css,scss} --fix",
    "_prepare": "your-script"
  },
  "dependencies": {
    "@aws-amplify/ui-vue": "^4.2.26",
    "@iconify/iconify": "3.1.1",
    "@iconify/vue": "^4.2.0",
    "@nuxt/devalue": "2.0.2",
    "@vercel/analytics": "^1.3.1",
    "@vueform/slider": "2.1.9",
    "@vueuse/components": "10.5.0",
    "@vueuse/core": "10.5.0",
    "@vueuse/integrations": "10.5.0",
    "amplify": "^0.0.11",
    "apexcharts": "^4.4.0",
    "aws-amplify": "^6.10.3",
    "axios": "1.6.0",
    "bulma": "0.9.4",
    "cobe": "0.6.3",
    "compression": "1.7.4",
    "date-fns": "2.30.0",
    "mapbox-gl": "2.15.0",
    "nprogress": "0.2.0",
    "pinia": "2.1.7",
    "plyr": "3.7.8",
    "prism-theme-vars": "0.2.4",
    "prismjs": "1.29.0",
    "tslib": "2.6.2",
    "universal-cookie": "6.1.1",
    "vue": "3.3.7",
    "vue-accessible-color-picker": "4.1.4",
    "vue-data-ui": "^2.4.42",
    "vue-demi": "0.14.6",
    "vue-i18n": "9.6.2",
    "vue-marquee-text-component": "2.0.1",
    "vue-my-photos": "3.0.1",
    "vue-prism-component": "2.0.0",
    "vue-scrollto": "2.20.0",
    "vue-virtual-scroller": "2.0.0-beta.8",
    "vue3-apexcharts": "^1.8.0",
    "vue3-carousel": "0.3.1",
    "vue3-markdown-it": "1.0.10",
    "vue3-popper": "1.5.0",
    "vue3-scroll-spy": "1.0.8"
  },
  "devDependencies": {
    "@aws-amplify/backend": "^1.9.0",
    "@aws-amplify/backend-cli": "^1.4.3",
    "@commitlint/cli": "18.2.0",
    "@commitlint/config-conventional": "18.1.0",
    "@cssninja/bulma-css-vars": "0.9.2",
    "@fontsource-variable/montserrat": "^5.1.0",
    "@fontsource-variable/roboto-flex": "^5.1.0",
    "@iconify/json": "2.2.135",
    "@intlify/unplugin-vue-i18n": "1.4.0",
    "@minify-html/node": "^0.11.5",
    "@types/compression": "^1.7.5",
    "@types/mapbox-gl": "2.7.17",
    "@types/node": "18.14.0",
    "@types/nprogress": "0.2.2",
    "@types/serve-static": "^1.15.7",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "@unhead/addons": "^1.11.6",
    "@unhead/ssr": "^1.11.6",
    "@unhead/vue": "^1.11.6",
    "@vitejs/plugin-vue": "4.4.0",
    "autoprefixer": "10.4.16",
    "aws-cdk": "^2",
    "aws-cdk-lib": "^2",
    "chalk": "^5.3.0",
    "commitlint": "18.2.0",
    "constructs": "^10.0.0",
    "cross-env": "7.0.3",
    "esbuild": "^0.24.0",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-frontmatter": "0.0.8",
    "eslint-plugin-md": "1.0.19",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-vue": "9.18.1",
    "eslint-plugin-vuejs-accessibility": "2.2.0",
    "esno": "0.17.0",
    "fast-glob": "^3.3.2",
    "h3": "1.8.2",
    "husky": "^4.3.8",
    "jiti": "1.21.0",
    "lint-staged": "15.0.2",
    "listhen": "1.5.5",
    "npm-run-all": "4.1.5",
    "postcss-html": "1.5.0",
    "postcss-scss": "4.0.9",
    "prettier": "3.0.3",
    "rimraf": "5.0.5",
    "sass": "1.69.5",
    "serve": "^14.2.3",
    "serve-static": "1.15.0",
    "stylelint": "15.11.0",
    "stylelint-config-recommended-vue": "1.5.0",
    "stylelint-config-standard": "34.0.0",
    "stylelint-config-standard-scss": "11.0.0",
    "stylelint-prettier": "4.0.2",
    "stylelint-scss": "5.3.0",
    "tsx": "^4.19.2",
    "typescript": "5.2.2",
    "unhead": "^1.11.6",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-fonts": "^1.1.1",
    "unplugin-icons": "0.17.1",
    "unplugin-vue-components": "0.25.2",
    "vite": "5.0.0-beta.14",
    "vite-plugin-pages": "0.31.0",
    "vite-plugin-pages-sitemap": "^1.7.1",
    "vite-plugin-purge-icons": "0.9.2",
    "vite-plugin-pwa": "0.16.6",
    "vite-plugin-radar": "0.9.1",
    "vue-router": "4.2.5",
    "vue-tsc": "1.8.22",
    "yorkie": "2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -e -V "
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint -c .eslintrc.cjs --fix"
    ],
    "*.scss": [
      "prettier --write",
      "stylelint"
    ],
    "*.md": [
      "prettier --write",
      "eslint -c .eslintrc.cjs --fix"
    ],
    "*.vue": [
      "prettier --write",
      "eslint -c .eslintrc.cjs --fix"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@types/markdown-it",
        "postcss",
        "stylelint"
      ]
    },
    "allowedVersions": {
      "vite": "5.0.0-beta.14"
    }
  }
}
